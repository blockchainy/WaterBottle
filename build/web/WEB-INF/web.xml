<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>VerifyLogin</servlet-name>
        <servlet-class>VerifyLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifySignup</servlet-name>
        <servlet-class>VerifySignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchWater</servlet-name>
        <servlet-class>SearchWater</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitReview</servlet-name>
        <servlet-class>SubmitReview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddItem</servlet-name>
        <servlet-class>AddItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayImages</servlet-name>
        <servlet-class>DisplayImages</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetId</servlet-name>
        <servlet-class>GetId</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayReviews</servlet-name>
        <servlet-class>DisplayReviews</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayImagesR</servlet-name>
        <servlet-class>DisplayImagesR</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchWaterR</servlet-name>
        <servlet-class>SearchWaterR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VerifyLogin</servlet-name>
        <url-pattern>/VerifyLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifySignup</servlet-name>
        <url-pattern>/VerifySignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchWater</servlet-name>
        <url-pattern>/SearchWater</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitReview</servlet-name>
        <url-pattern>/SubmitReview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddItem</servlet-name>
        <url-pattern>/AddItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayImages</servlet-name>
        <url-pattern>/DisplayImages</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetId</servlet-name>
        <url-pattern>/GetId</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayReviews</servlet-name>
        <url-pattern>/DisplayReviews</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayImagesR</servlet-name>
        <url-pattern>/DisplayImagesR</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchWaterR</servlet-name>
        <url-pattern>/SearchWaterR</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
